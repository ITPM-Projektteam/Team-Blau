unit KlasseVektorBerechnung;

interface

function AVektor(NVektor, OVektor: Array[1..2] of Integer): Array[1..2] of Integer;
function SVektor(NVektor, OVektor: Array[1..2] of Integer): Array[1..2] of Integer;
function MVektor(NVektor, OVektor: Array[1..2] of Integer): Array[1..2] of Integer;
function DVektor(NVektor, OVektor: Array[1..2] of Integer): Array[1..2] of Integer;

implementation

end.
